      <providers>
        <clear />
        <add name="MyProfileProvider" type="Altairis.Web.Security.SimpleSqlProfileProvider" connectionStringName="BugNET" tableName="BugNet_UserProfiles" keyColumnName="UserName" lastUpdateColumnName="LastUpdate" />
      </providers>
